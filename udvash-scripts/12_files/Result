        function gettoken() {
                var token = '<input name="__RequestVerificationToken" type="hidden" value="J1CtCPUjD2V4Wcpzy485_dJqxRV4_I6KdzmEwn5HjeIZrvzvakayFhivvpVzqnV9QCCOCkd4zEMMfllRVGtkRv0zDkN7oWHXJdNhDZbR8JA1" />';
