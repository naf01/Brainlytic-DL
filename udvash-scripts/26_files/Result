    <script type="text/javascript">
        $(document).ready(function () {
            antyForgery = gettoken();

            let checkedCategories = [];
            $(document).on('change', '.filter-checkbox input[type="checkbox"]', function () {
                let checkboxValues = $(this).parent().attr('data-category').split(',');
                if ($(this).is(':checked')) {
                    checkboxValues.forEach(function (value) {
                        value = value.trim();
                        if (!checkedCategories.includes(value)) {
                            checkedCategories.push(value);
                        }
                    });
                } else {
                    checkboxValues.forEach(function (value) {
                        value = value.trim();
                        let index = checkedCategories.indexOf(value);
                        if (index > -1) {
                            checkedCategories.splice(index, 1);
                        }
                    });
                }
                $('.mcq-exam-body .questionBlock').each(function () {
                    let questionValues = $(this).attr('data-category').split(',');
                    let showBlock = false;
                    questionValues.forEach(function (value) {
                        value = value.trim();
                        if (checkedCategories.includes(value)) {
                            showBlock = true;
                        }
                    });

                    if (checkedCategories.length === 0 || showBlock) {
                        $(this).removeClass('d-none');
                    } else {
                        $(this).addClass('d-none');
                    }
                });
            });
        });
    </script>

</body>
</html>
