        function gettoken() {
                var token = '<input name="__RequestVerificationToken" type="hidden" value="nlHuJFu-_tXl_fwxxyk8RgzyRauDBxcNCVTtfslq6A-NP5rqFa-IYrGGpBpOp-m2BUKuf2E5H5H1ZSao3qYLo5gmiB7oMf9FXXofF0T9vcA1" />';
