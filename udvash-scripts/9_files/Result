        function gettoken() {
                var token = '<input name="__RequestVerificationToken" type="hidden" value="EZLJsaNwD_m-x_d3Awbdai_4DcFoEAveu08sx0XQTJ3SX1efyAAh5LwR6Hje3IS08rOvdxE6_dT_stroq8ytiAhwGjer7Ej8mSorbXK6_Vg1" />';
                token = $(token).val();
                return token;
        }

        MathJax = {
            chtml: {
                mtextInheritFont: true,
            }
